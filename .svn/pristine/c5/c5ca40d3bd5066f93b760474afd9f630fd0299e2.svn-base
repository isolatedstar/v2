body {

  background-position: initial initial;
  background-repeat: initial initial;
  background-position: initial initial;
  background-repeat: initial initial;
  margin: 0px;
  padding: 0px;
}
/*写的样式*/
*{margin: 0;padding: 0;}
/*Navbar panel标题栏的阴影框*/
.box-shodm{ -moz-border-bottom-colors: none;-moz-border-left-colors: none; -moz-border-right-colors: none;-moz-border-top-colors: none;
 background: rgba(0, 0, 0, 0) linear-gradient(to bottom, rgba(255, 255, 255, 0) 0%, rgba(0, 0, 0, 0.1) 100%) repeat scroll 0 0;
 border-color: -moz-use-text-color -moz-use-text-color #dedede;border-image: none;font-size: 15px;font-weight: bold;cursor: move;
} 
/*右侧布局*/
.panel-table-body {
    padding:0px 15px;
}
/*panel左侧和右侧的阴影*/
.article-bj{border:1px solid #DDDDDD; box-shadow:6px 6px 10px rgba(0,0,0,0.5);}
/*input框样式*/
.input-group{margin-bottom: 5px;}
/*表格中input框的样式*/
.input-size {background-color: #fff; background-image: none; border: 1px solid #ccc;width: 150px;
border-radius: 4px;box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;color: #555;font-size: 14px;line-height: 1.42857;
padding: 6px 12px;transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
}
.float-right{float: right;}
/*表单边距*/
.form-margin{margin:15px;}
/*查询按钮样式*/
.cx-btn-margin{/*padding: 10px 15px;*/margin-right:15px;box-sizing: border-box;}
/*表单label外边距*/
.h5-margin{float: right;width:80px;padding-right:10px;box-sizing: border-box;text-align: right;display: inline-block;margin-right:25px;}
/*表单label左边距*/
.h5-margin-left{text-align: left;margin-left:11px;width:93px;}
h4{color:#337AB7; margin-left:10px; margin-top:15px}
/*表单input-group右边距，可以控制其中input大小*/
.input-group-btn{padding-right:10px;}
/* 弹出弹出框控制距离的高度*/
.tanchu-box{width:90%;height:auto;margin:100px auto;} 
/* 弹出弹出框（中等的）*/
.m-tanchu-box{width:70%;height:auto;margin:100px auto;} 
/* 弹出弹出框（小的）*/
.s-tanchu-box{width:50%;height:auto;margin:100px auto;} 
/*页脚*/
footer p{text-align: center;}
/*页脚*/
/*杨蕊*/
.ah2_btn_margin{
	margin-left:15px;
	margin-top:12px;}
/*适应黑色商城框架的 容器边距*/
.container-margin{
	margin-top:15px;
	}
/*树tree-view小图标样式*/
@font-face {
  font-family: 'Glyphicons Halflings';
  src: url('../fonts/glyphicons-halflings-regular.eot');
  src: url('../fonts/glyphicons-halflings-regular.eot?#iefix') format('embedded-opentype'), url('../fonts/glyphicons-halflings-regular.woff') format('woff'), url('../fonts/glyphicons-halflings-regular.ttf') format('truetype'), url('../fonts/glyphicons-halflings-regular.svg#glyphicons_halflingsregular') format('svg');
}
.glyphicon {
  padding-right: 15px;
  position: relative;
  top: 1px;
  display: inline-block;
  font-family: 'Glyphicons Halflings';
  -webkit-font-smoothing: antialiased;
  font-style: normal;
  font-weight: normal;
  line-height: 1;
  -moz-osx-font-smoothing: grayscale;
}
