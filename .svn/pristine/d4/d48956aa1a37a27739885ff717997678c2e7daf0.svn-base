@charset "utf-8";
/* CSS Document */
*{ padding:0; margin:0; } li{ list-style:none;} a{ text-decoration:none;} img{ border:none;} input,select,textarea{ outline:none;}
body{ font-family:"微软雅黑", "宋体"; color:#333; background:#f2f2f2;}
input{ background-color:transparent; border:0;  vertical-align:top;}
.clear{clear:both;}
#mallwarp{ width:1280px; height:auto; margin:0 auto; background:#fff; }
/*********************************************header start************************************************************/
.m_header{ width:100%; height:79px; border-bottom:1px #ddd solid; background:#fff;}
.m_header>img{ width:130px; height:60px; margin:10px 0 0 20px; float:left;}
.m_header>h2{ display:inline; font-size:30px; color:#f60200; padding-left:20px; line-height:100px;}
/*********************************************header over************************************************************/
/*********************************************conter top start***************************************************************/
.m_con_box{width:1280px; height:50px; position:relative;}
.m_content_top{height:50px;}
.m_con_top_p{ width:400px;font-size:14px; margin-left:16%; float:left; position:relative;}
.m_con_top_p select{width:100px; height:30px;margin-left:10px; border:1px #ddd solid; margin-right:26px;}
.m_con_top_p label{ font-weight:bold; padding-left:20px;}
.m_con_top_p2{ color:#333; display:inline-block; width:100px; height:32px; text-align:center; border:1px #ddd solid; line-height:32px; background:#fff; margin-left:3%; margin-top:6px; position:relative;}
.m_con_top_p2 i{ font-size:16px; line-height:30px; padding-left:10px;}
.m_con_top_p2:hover{ background:#f60200; color:#fff;}
.m_sech_form{ width:500px; float:right; padding-top:8px; padding-right:20px;}
.m_sech_text{ width:300px; height:32px; border:1px #f60200 solid; padding-left:10px; color:#ccc; background:#fff;}
.m_sech_btn{ width:80px; height:32px; background:#f60200; color:#fff; font-size:14px;}
.con_shapping_box{ float:right;}
.con_shapping_box>i{ padding:8px 10px; font-size:14px;  background:#fff; border:1px #f60200 solid; color:#f60200; line-height:14px;}
.con_shapping_box>span{ font-size:20px;}
/*********************************************conter top  over***************************************************************/
/*********************************************nav start**************************************************************/
.m_nav{ position:absolute; left:0; top:0; z-index:99999;}
.m_menu{ width:181px; height:40px; background-color:#f60200;color:#fff; font-size:18px; font-weight:bold;line-height:38px; }
.m_menu>p{ text-align:center;}
.m_menu i{ font-size:18px; padding-left:20px;}
.m_menu_2 { display:none;}
.m_menu_2 i{ font-size:34px; color:#f60200; padding:6px 0 0 20px;}
.m_menu_ul{ width:180px; height:auto; border-left:1px #ddd solid; border-right:1px #ddd solid; display:none; background:#fff;}
.m_menu_ul>li{width:180px; height:40px; border-bottom:1px #ddd solid;}
.m_menu_ul>li>a{ display:block; font-size:16px; line-height:38px; color:#333; padding-left:48px; background:url(../images/navbj_03.png) no-repeat right top; margin-right:10px; }
.m_menu_ul>li>a:hover{ color:#f60200;}
.m_subnav{ border-bottom:1px #ddd solid; display:none; background:#fff;}
.m_subnav>li{width:180px; height:40px;}
.m_subnav>li>a{display:block; font-size:16px; line-height:38px; color:#333; padding-left:70px;  }
.m_subnav>li>a:hover{ color:#f60200; }
/*********************************************nav over***************************************************************/
.content_box_warp{width:1280px; height:auto; background:#fff; margin-top:15px;  }
.content_div{ width:240px; height:auto; border:1px #e4e4e4 solid; background:#fff; box-shadow:0 0 6px rgba(0,0,0,0.1); float:left; margin:0 7px; margin-bottom:12px;}
.content_div_img{ width:240px; height:200px; overflow:hidden;}
.content_div_img>img{ -moz-transition:all 0.2s ease;-webkit-transition:all 0.2s ease; -ms-transition:all 0.2s ease; }
.content_div_img>img:hover{transform:scale(1.1); -webkit-transform:scale(1.1); -ms-transform:scale(1.1);}
.content_div_txt{ height:auto;}
.content_div_p1{ font-size:14px; color:#333; padding-left:5px; line-height:40px;}
.content_div_p1>span{ font-size:18px;}
.content_div_p2{ font-size:14px; color:#333; padding-left:6px;}
.content_div_p2>a{ color:#ff0000;}
.content_div_p2>a>span{ border-bottom:1px solid #ff0000;}
.content_div_p3{ font-size:13px; color:#333; padding-left:6px; line-height:20px;}
.content_div_p4{ font-size:13px; color:#333; padding-left:6px; margin:0;}
.content_div p:hover{ color:#ff6600;}
.content_div_p5{color:#ff0000; font_size:13px; line-height:26px; padding:0 10px 5px 0; cursor:pointer; float:right;}

.div_p4_span{ font-size:14px; font-weight:bold;}
.content_2{padding-top:30px;}
.content_2>p{ font-size:16px; color:#333; line-height:40px; width:1260px; height:40px; background:#f2f7ff; padding:40px 20px; margin-bottom:10px;}
.content_2ul{ display:none; position:absolute;left:226px; top:50px; width:200px; height:auto; background:#eee; padding-left:20px; z-index:999999; border-radius:4px;}
.content_2ul li{ font-size:14px; line-height:30px; }
.content_2ul li input{margin-top:9px; margin-right:9px;}

.content_3_warp{width:800px; height:auto; margin:0 auto;}
.content_3_box{width:340px; height:auto; border:1px #ccc solid; float:left;}
.content_3_box_in{width:300px; height:auto; margin:0 auto; padding-top:20px;}
.content_3_txt p{ font-size:16px; line-height:32px;}
.content_3_bot{border-top:1px #ccc dashed; padding-bottom:20px;}
.content_3_bot p{ font-size:16px; line-height:34px;}

.footer{ height:auto; background:#eee;}

.footer_in_bot{width:890px; margin-left:140px; padding-top:30px;}
.footer_in_bot>p{ font-size:12px; color:#000; line-height:32px; text-align:center;}
.footer_in_bot>p a{color:#000;}
#footer a:hover{ color:#067af4;}
.footer_in_bot>p:hover{ color:#067af4;}
.footer_in_ul{padding-left:124px;}
.footer_in_ul span{ padding:0 4px;}
.footer_in_ul li{ float:left;font-size:12px;line-height:30px; }
.footer_in_ul li a{ color:#000;}
.footer_in_ul li:hover{color:#067af4;}



/*详情页 轮播图 CSS*/
 .slider-extra { 
  width: 100%; 
  height: 400px; 
  border: 1px solid #eee
 } 
 .slider { 
  text-align: center; 
  margin: 30px auto; 
  position: relative; 
 } 
 .slider-panel, .slider-nav, .slider-pre, .slider-next { 
  position: absolute; 
  z-index: 8; 
 } 
 .slider-panel{
  width:100%;height: 400px;
     display: table-cell;
 vertical-align:middle;
 }
 .slider-panel a{ 
    display: table-cell;
vertical-align:middle;
/*设置水平居中*/
width: 2000px;height:400px;
text-align:center;
/* 针对IE的Hack */
font-family:Arial;/*防止非utf-8引起的hack失效问题，如gbk编码*/
 } 
 .slider-panel img { 
  max-width: 600px;
  max-height:400px;
} 
.slider-extra { 
  position: relative; 
 } 
 .slider-nav {
  width:100%;
  margin: 0  auto; 
  margin-top:355px;
 } 
 .slider-nav li { 
  background: #3e3e3e; 
  border-radius: 50%; 
  color: #fff; 
  cursor: pointer; 
  margin: 0 2px; 
  overflow: hidden; 
  text-align: center; 
  display: inline-block; 
  height: 18px; 
  line-height: 18px; 
  width: 18px; 
 } 
 .slider-nav .slider-item-selected { 
  background: #f60200; 
 } 
 .slider-page a{ 
  background: rgba(0, 0, 0, 0.2); 
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#33000000,endColorstr=#33000000); 
  color: #fff; 
  text-align: center; 
  display: block; 
  font-family: "simsun"; 
  font-size: 22px; 
  width: 28px; 
  height: 62px; 
  line-height: 62px; 
  margin-top: -31px; 
  position: absolute; 
  top: 50%; 
 } 
 .slider-page a:HOVER { 
  background: rgba(0, 0, 0, 0.4); 
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#66000000,endColorstr=#66000000); 
 } 
 .slider-next { 
  left: 100%; 
  margin-left: -28px; 
 } 
 
 
