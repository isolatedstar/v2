@charset "utf-8";
/* CSS Document */
/*main top*/
#main{width:1202px;height:auto;margin:0 auto;}
.main_top{height:235px; background:#fff6e4; margin:82px 0; position:relative;}
.main_top_left{width:254px; height:231px;border:2px solid #e29f06; background:#fff; float:left;}
.main_top_cen{width:572px; float:left; color:#828383;}
.main_top_cen h2{font-size:18px; font-weight:normal; line-height:77px; padding-top:10px;text-align:center; }
.main_top_cen p{line-height:24px; padding-left:30px; padding-right:10px;}
.main_top_right{width:374px; height:272px; position:absolute; right:0; top:-22px; z-index:1; background:url(../images/erweima_05.png) no-repeat right top;}
.main_top_right h2{ font-size:24px; color:#ecbb51; width:5px; position:absolute; left:76px; top:102px;}
.main_top_right img{ position:absolute; left:135px; top:40px;}
/*main zs*/
.main_zs_title{color:#828383; padding-bottom:100px;}
.main_zs_title h2{width:474px; float:left; padding-left:536px; line-height:30px; font-size:30px; font-weight:normal;}
.main_zs_title span{width:192px; float:right; line-height:30px; font-size:18px;}
.main_zs_title span a{color:#828383;}
.main_zs_title span a:hover{ color:#e7a519;}
.main_zs_tu{padding:0 56px;}
.main_zs_tu1{width:359px; height:271px; margin-bottom:64px; float:left;}
.zs_tu1_img{width:325px;height:216px; border:1px #cbcbcb solid; margin:0 auto; overflow:hidden;-moz-transition:all 0.2s ease;-webkit-transition:all 0.2s ease; -ms-transition:all 0.2s ease; }
.zs_tu1_img img{width:325px;height:216px; -moz-transition:all 0.2s ease;-webkit-transition:all 0.2s ease; -ms-transition:all 0.2s ease; }
.zs_tu1_img img:hover{transform:scale(1.3); -webkit-transform:scale(1.3); -ms-transform:scale(1.3);}
.zs_tu1_img:hover{ box-shadow:0 0 20px rgba(0,0,0,0.1);transform:translate(0,-1em);}
.main_zs_tu1 p{ text-align:center; color:#979292; font-size:18px; padding-top:32px;}
.main_zs_tu1 p span{ font-size:24px; color:#f01313; padding-left:52px;}
.Quality_title{width:1093px; height:53px; border-bottom:2px #ebba51 solid; margin:0 auto; color:#828383;}
.Quality_title h2{ font-size:24px;  font-weight:normal; line-height:53px; padding-left:37px; position:relative; float:left;}
.Quality_title h2:before{ content:""; display:block; width:13px; height:33px; background:#ecbb52; position:absolute; left:10px; top:8px;}
.Quality_title span{ float:right; font-size:18px; line-height:58px; padding-top:4px; padding-right:57px;}
.Quality_title span a{color:#828383;}
.Quality_title span a:hover{color:#e7a519;}
.Quality_tu{width:auto; height:auto;margin-top:59px; margin-left:56px; margin-bottom:52px;}
.Quality_tu1{width:200px; height:220px; margin:0 36px; float:left;-moz-transition:all 1s ease;-webkit-transition:all 1s ease;
 -ms-transition:all 1s ease;margin-bottom:20px; }
.Quality_tu1 img{width:200px; height:220px; }
.Quality_tu1 img:hover{transform:scale(1.5); -webkit-transform:scale(1.5); -ms-transform:scale(1.5); box-shadow:0 0 10px rgba(0,0,0,0.3)}
.main_bot{margin-left:52px;}
.main_bot p{ float:left; line-height:234px; width:286px;}
.main_bot p span{ font-size:29px; color:#fff; display:inline-block; width:47px; height:53px; background:url(../images/qyzz_bj_03.png) no-repeat left center; text-align:center; line-height:50px;}
.main_bot p a{ font-size:20px; color:#323232; padding-left:6px;}
/*tel main*/
#tel_main{width:1198px; height:auto; margin:0 auto;}
.tel_main_top{height:260px; background:#fff6e4; margin:78px 0 94px 0;}
.tel_top_left{width:280px; height:256px; border:2px #e49d03 solid; background:#fff; float:left;}
.tel_top_left img{width:280px; height:256px;}
.tel_top_title{width:914px; float:left; color:#828383;}
.tel_top_title h2{font-size:24px; font-weight:normal; line-height:93px; padding-top:2px;text-align:center;}
.tel_top_title p{ line-height:32px; font-size:18px; padding:0 37px 0 28px;}
/*main bot*/
.tel_main_bot{margin-bottom:123px;}
.tel_bot_title{height:47px; background:#e49d04;}
.tel_bot_title h2{ font-size:18px; color:#fff; line-height:47px; padding-left:29px; background:url(../images/tel_bot_title_bj_03.png) no-repeat left center;}
.tel_bot_con{height:418px; background:#fff6e3;}
.tel_bot_con h2{ font-size:18px; color:#828383; line-height:76px; padding-left:60px; font-weight:normal;}
table{width:1108px; height:280px; margin:0 auto; background:#fff;}
table tr td{border-top:2px #8c8c8b solid; border-left:2px #8c8c8b solid; font-size:18px; color:#828383; padding-left:25px;}

























































































































