.fac_wrap{width:1204px; height:auto; margin:0 auto;}
/* head */
.fac_head_all{width:1204px; height:80px;}
.fac_head{width:1202px; margin:0 auto; height:78px; background:#afd6fa; border:1px solid #ccc;}
.fac_hleft{float:left; margin:25px 0 0 85px;}
.fac_hleft img{margin:-20px 20px 0 0;}
.fac_hleft .fac_title{color:#0099ff; font-weight:700; font-family: "汉仪秀英体简 Bold","汉仪秀英体简"; font-size:40px;font-style:normal;}
.fac_hright{float:right; margin:55px 85px 0 0;}
.fac_hright p{display:block; float:left; margin:0 0 0 20px;}
/* cont */
#fac_cont{width:1204px; height:auto !important; background:#f2f2f2; padding:30px 0;}
#fac_cont:after{ content:"."; height:0; visibility:hidden; display:block; clear:both;} 
/*  menu */
#fac_menu{float:left;width:150px; height:auto;padding:10px;margin-left:5px;}
#fac_menu .fac_menu_top{ width:150px; height:65px; background:#cce4f9; font-size:20px; text-align:center; line-height:65px; border:1px #ccc solid; border-bottom:none;
 							text-shadow:2px 2px 1px rgba(0,0,0,0.2);}
#fac_menu ul{ border-bottom:1px solid #ccc;}
#fac_menu li{cursor:pointer; width:150px; height:40px; border:1px solid #ccc; border-bottom:none; color:#000;  display:block; color:#000; line-height:40px; text-align:center;  background:#f0f5f9;}

.bgs{background:red; color:red;}
#fac_menu .fac_menu_one{background:#3366ff;}
#fac_cf_ul .fac_cfbg{background:#e1ebf4;}
#fac_menu .content{clear:both; width:240px; height:80px; background:#6e6e6e; padding:20px; color:#fff;}
/* right_cont */
.fac_content{float:right;width:980px; height:800px; margin:20px 20px 0 0; overflow:hidden;}
/* bottom */

/* login */
.login_wrap{width:1204px; height:704px; margin:0 auto; background:url(../images/baol_bg.png) no-repeat;}
.login_div{position:relative;top:300px; left:600px;}
.login_div li{display:block; margin-bottom:15px;}
.login_div li h1{font-size:20px; font-family:"黑体"; font-weight:700;}
.login_div li:nth-child(2),.login_div li:nth-child(3){width:260px;height:36px; line-height:36px;border:1px solid #e2e3ea; border-top:1px solid #abadb3;text-indent:0.3em;background:#fff;}
.login_div li input{border:none; width:215px;}
.login_div li img{float:right; margin:5px 6px 0 0;}
.login_div button{width:90px; height:40px; line-height:40px; color:#fff;font-size:18px; background:#73add2; border-radius:5px;margin-left:152px;}

/* 页面导航 */
.fac_wnav{width:860px; height:600px;margin:0 auto;}
.fac_wnav01{width:860px; height:auto; margin-top:100px;}
.fac_wnav01 li{display:block; float:left; width:100px;cursor:pointer;padding:0px 10px 5px; border:1px solid #797979;background:#477097;border-radius:10px;text-align:center; margin:0 45px;}
.fac_wnav01 li a{color:#fff;}
.fac_wnav01 li:hover{background:#3366ff;}
.fac_wnav01 li img{margin:8px 0;}

