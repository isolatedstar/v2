package com.zllh.mall.common.enums;

public enum PayGetTypeEnum {

    //0.付款方 1.收款方
	/** 付款方 */
	PAY(0, "付款方"),

	/** 收款方 */
	GET(1, "收款方");

	private String text;

	private Integer value;

	private PayGetTypeEnum(Integer value, String text) {
		this.text = text;
		this.value = value;
	}

	public String getText() {
		return text;
	}

	public Integer getValue() {
		return value;
	}
}
