.db_query li{display:block; float:left;}
.db_query li:nth-child(2){margin-left:60px;}
.db_query select{width:150px; height:28px;}
.db_query li input{height:24px;}
.fac_fanh{float:right; margin-top:30px; margin-right:10px;}
/*担保登记10001内页*/
.inner_a_box{width:800px; height:170px; margin:0 auto; border:1px #999 solid;background:#5fb7f6; margin:80px 0 80px 100px; position:relative; border-radius:0 10px 0 0; display:none;}
.inner_a_box_content{margin:40px;}
.pay_button_a_in{ position:absolute; top:-1px; right:-6px; border:1px #797979 solid;}
/*担保登记10002内页*/
