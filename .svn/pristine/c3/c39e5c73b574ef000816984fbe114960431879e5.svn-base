			</if>
			<if test="bankId != null">
				BANK_ID = #{bankId,jdbcType=VARCHAR},
			</if>
			<if test="masterAcct == ''">
				MASTER_ACCT = null,
			</if>
			<if test="masterAcct != ''">
				MASTER_ACCT = #{masterAcct,jdbcType=VARCHAR},
			</if>
			<if test="isDefault != null">
				IS_DEFAULT = #{isDefault,jdbcType=TINYINT},
			</if>
		</set>
		where BANK_ACCT = #{bankAcct,jdbcType=VARCHAR}
