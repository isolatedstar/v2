@charset "utf-8";
/* CSS Document */
@media screen and (max-width:980px){
	.m_sech_form{padding-right:28%; margin:20px 0;}
	.content_box_warp{ width:100%; margin:0 auto;}
	.content_div{ width:100%; height:200px;}
	.content_div_img{float:left;}
	.content_div_txt{float:left; padding-left:20px;}
	.content_div_p1{ font-size:16px; line-height:80px;}
	.content_div_p3{ font-size:16px; }
	.con_seach_btn { font-size:14px;}
	.footer,.m_header{width:980px;}
	.footer_in_bot{margin-left:10px; padding-bottom:20px;}
	.content_3_warp{width:980px; margin-left:-1px;}
	}
@media screen and (max-width:750px){
	.m_sech_form{padding-right:43%; margin:20px 0;}
	.content_div{margin-left:2px;}
	.content_3_warp{width:750px; margin-left:-1px;}
	.content_3_txt{width:700px;}
	.content_3_bot{width:700px;}
	 .slider,.slider-extra{width:700px;}
	 .m_header,.footer{width:750px;}
	.footer_in_bot{width:750px; margin-left:0; padding-bottom:20px;}
	.footer_in_ul{padding-left:0;}
	.footer_in_ul li:nth-child(1){ padding-left:53px;}
	
	.m_menu i{display:none;}
	.m_menu{ font-size:32px; width:221px; height:64px; line-height:64px;}
	.content_box_warp{margin-top:40px;}
	.m_menu_ul{width:220px;}
	.m_menu_ul>li{width:220px; height:56px;}
	.m_menu_ul>li>a{font-size:28px; line-height:56px; background-image:none;}
	.m_subnav>li{width:220px; height:56px; }
	.m_subnav>li>a{font-size:28px; line-height:56px; }
	.m_subnav_2 li{width:220px; height:56px;}
	.m_subnav_2 li a{ font-size:28px; line-height:56px;}


	}
@media screen and (max-width:480px){
	}